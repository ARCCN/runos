{

"default": {
    "services": [
        "learning-switch",
        "link-discovery",
        "rest-listener",
        "host-manager",
        "webui",
        "static-flow-pusher",
        "switch-stats",
        "stp",
        "flow-manager",
        "rest-multipart",
        "maple",
        "command-line-interface",
        "rest-flowmod"
    ],

    "tables": {
        "static-flow-pusher" : 0,
        "maple" : 1
    },

    "flow-manager" : {
        "interval" : 5
    },

    "rest-listener" : {
         "port" : 8000,
         "web-dir" : "./build/web"
    },

    "controller": {
         "nthreads": 1,
         "cbench": false
   },

    "maple": {
          "pipeline": [
             "link-discovery",
             "host-manager",
             "forwarding"
         ]
    },

    "loader": {
        "threads": 2
    },

    "link-discovery": {
        "poll-interval": 10,
        "pin-to-thread": 1
    },

    "switch-manager": {
        "pin-to-thread": 1
    },

    "switch-stats": {
	"poll-interval": 1,
	"pin-to-thread": 1
    }
}

}
