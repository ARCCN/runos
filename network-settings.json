{

"default": {
    "services": [
        "learning-switch",
        "rest-listener",
        "host-manager",
        "path-verifier",
        "webui",
        "switch-stats",
        "stp",
        "static-flow-pusher",
        "arp-handler",
        "flow-manager",
        "acl"
    ],

    "rest-listener" : {
         "port" : 8000
    },

    "controller": {
         "nthreads": 4,
         "cbench": false
    },

    "loader": {
        "threads": 2
    },

    "link-discovery": {
        "poll-interval": 10,
        "pin-to-thread": 1
    },

    "switch-manager": {
        "pin-to-thread": 1
    },

    "switch-stats": {
	"poll-interval": 1,
	"pin-to-thread": 1
    },

    "acl" : {
        "rules" : [
            {"action" : "drop", "proto" : "tcp", "src" : {"ip" : "10.0.0.2", "mac" : "any", "port" : "any"}, "dst" : {"ip" : "10.0.0.3", "mac" : "any", "port" : "any"}, "flow_limit" : "any"},
            {"action" : "drop", "proto" : "udp", "src" : {"ip" : "10.0.0.2", "mac" : "any", "port" : "any"}, "dst" : {"ip" : "10.0.0.3", "mac" : "any", "port" : "any"}, "flow-limit" : "any"},
            {"action" : "drop", "proto" : "udp", "src" : {"ip" : "10.1.0.0/255.255.255.0", "mac" : "any", "port" : 80}, "dst" : {"ip" : "any", "mac" : "any", "port" : 80}, "flow_limit" : "any"}
        ]
    }
}

}
